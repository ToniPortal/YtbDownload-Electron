{
  "name": "ytbto",
  "version": "1.1.0",
  "description": "Une application faite avec eleclectron qui utulise le packet ytdl-core pour télécharger une vidéo youtube !",
  "main": "index.js",
  "scripts": {
    "test": "electron .",
    "start": "electron .",
    "build": "npx electron-packager ./ YtbDownload-Electron --no-prune --app-version='1.1.0' --app-copyright --platform=win32 --arch=x64 --overwrite --icon=icon.ico --prune=true --out=release-builds --executable-name='YtbDownload-Electron' --win32metadata.CompanyName='YtbDownload-Electron' --win32metadata.FileDescription='YtbDownload-Electron' --win32metadata.ProductName='YtbDownload-Electron' --ignore='icon.ico' --ignore='./src/*' --ignore='README.md' --ignore='./download/video.mp4' --ignore='release-builds'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bouftout/ytbtoall.git"
  },
  "author": "ToniPortal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bouftout/ytbtoall/issues"
  },
  "homepage": "https://github.com/Bouftout/ytbtoall#readme",
  "devDependencies": {
    "electron": "^22.1.0",
    "electron-packager": "^14.2.1"
  },  
  "dependencies": {
    "discord-rpc": "^4.0.1",
    "ffmpeg-static": "^5.2.0",
    "ytdl-core": "^4.11.5"
  }
}
